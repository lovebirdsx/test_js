{
    "editor.detectIndentation": false,
    "filewatcher.commands": [
        {
            "match": "\\.js",
            "event": "onFileChange",
            "cmd": "node ${file}"
        },
        {
            "match": "\\.ts",
            "event": "onFileChange",
            "cmd": "ts-node ${file}"
        }
    ],
    "filewatcher.autoClearConsole": true
}