{
    "editor.tabSize": 2,
    "[typescript]": {
        "editor.codeLens": true
    },
    "[javascript]": {
        "editor.codeLens": true
    },
    "[javascriptreact]": {
        "editor.codeLens": true
    },
    "[typescriptreact]": {
        "editor.codeLens": true
    },
    "typescript.referencesCodeLens.enabled": false,
    "typescript.implementationsCodeLens.enabled": false,
    "jestrunner.jestCommand": "node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js",
    "jestrunner.debugOptions": {
        "runtimeArgs": ["--no-warnings", "--experimental-vm-modules"],
    },
    "jestrunner.disableCodeLens": false,
    "jestrunner.runOptions": [
        "--detectOpenHandles",
    ],
    "workbench.colorCustomizations": {
        "activityBar.activeBackground": "#5b1942",
        "activityBar.background": "#5b1942",
        "activityBar.foreground": "#e7e7e7",
        "activityBar.inactiveForeground": "#e7e7e799",
        "activityBarBadge.background": "#5b7d22",
        "activityBarBadge.foreground": "#e7e7e7",
        "commandCenter.border": "#e7e7e799",
        "sash.hoverBorder": "#5b1942",
        "statusBar.background": "#330e25",
        "statusBar.foreground": "#e7e7e7",
        "statusBarItem.hoverBackground": "#5b1942",
        "statusBarItem.remoteBackground": "#330e25",
        "statusBarItem.remoteForeground": "#e7e7e7",
        "titleBar.activeBackground": "#330e25",
        "titleBar.activeForeground": "#e7e7e7",
        "titleBar.inactiveBackground": "#330e2599",
        "titleBar.inactiveForeground": "#e7e7e799"
    },
    "typescript.tsdk": "node_modules/typescript/lib",
}