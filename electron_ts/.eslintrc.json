{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:import/electron",
    "plugin:import/typescript",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "rules": {
    "no-tabs": "error",
    "no-trailing-spaces": "error",
    "prettier/prettier": "error"
  }
}